{
  "name": "gtp",
  "version": "2.0.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/gtp.git"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib coverage doc",
    "lint": "tslint --project .",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "karma start karma.conf.js --single-run",
    "test:all-browsers": "karma start karma.conf.js --single-run --browsers Chrome,Firefox,PhantomJS",
    "test:watch": "karma start karma.conf.js",
    "doc": "typedoc --target ES5 --includeDeclarations --excludeExternals --mode modules --module commonjs --out doc/ --name gtp --exclude **/*.spec.ts src/",
    "upload-coverage": "cat coverage/P*/lcov.info | coveralls"
  },
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@types/jasmine": "3.3.12",
    "coveralls": "3.0.9",
    "jasmine-ajax": "4.0.0",
    "jasmine-core": "3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-commonjs": "1.0.0",
    "karma-coverage": "1.1.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-ajax": "0.1.13",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "phantomjs-prebuilt": "2.1.16",
    "rimraf": "3.0.0",
    "tslint": "5.16.0",
    "typedoc": "0.16.4",
    "typescript": "3.7.4"
  }
}
