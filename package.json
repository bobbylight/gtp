{
  "name": "gtp",
  "version": "2.0.6",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bobbylight/gtp.git"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib coverage doc",
    "lint": "eslint --ext .ts src/",
    "build": "tsc",
    "watch": "tsc --watch",
    "test": "karma start karma.conf.js --single-run",
    "test:all-browsers": "karma start karma.conf.js --single-run --browsers Chrome,Firefox,PhantomJS",
    "test:watch": "karma start karma.conf.js",
    "doc": "typedoc --target ES5 --includeDeclarations --excludeExternals --mode modules --module commonjs --out doc/ --name gtp --exclude **/*.spec.ts src/"
  },
  "files": [
    "lib/"
  ],
  "devDependencies": {
    "@types/jasmine": "3.7.7",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "eslint": "^7.29.0",
    "jasmine-ajax": "4.0.0",
    "jasmine-core": "3.7.1",
    "karma": "6.3.4",
    "karma-chrome-launcher": "3.1.0",
    "karma-commonjs": "1.0.0",
    "karma-coverage": "2.0.1",
    "karma-firefox-launcher": "2.1.1",
    "karma-ie-launcher": "1.0.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-ajax": "0.1.13",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-spec-reporter": "0.0.32",
    "phantomjs-prebuilt": "2.1.16",
    "rimraf": "3.0.2",
    "typedoc": "0.16.11",
    "typescript": "^3.8.3"
  }
}
