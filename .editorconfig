# A minimal configuration for some of the formatting basics enforced by tslint.

# This is the top-most EditorConfig file
root = true

[*.ts]
indent_style=tab
indent_size=4
end_of_line=lf
trim_trailing_whitespace=true
charset=utf-8
insert_final_newline=true

[*.json]
indent_style=space
indent_size=2
end_of_line=lf
charset=utf-8
