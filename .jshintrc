{
   "bitwise": true,
   "browser": true,
   "curly": true,        // Require curly braces around code blocks
   "debug": false,
   "devel": true,
   "eqeqeq": true,
   "latedef": true,      // Prohibit use of variable before it is defined
   "laxcomma": true,     // Suppress warnings about comma-first style in object literals
   "newcap": true,       // Require constructor functions to be capitalized
   "shadow": false,
   "strict": true,
   "undef": true,        // Warns about unknown globals
   "unused": "vars",     // Warn on unused variables (explicitly vars, not function params)
   
   // TODO: Move globals and exporteds into Gruntfile?
   "globals": {
      "gtp": true,
      "game": true,
      "_BaseState": true,
      "BattleEntity": true,
      "Brecconary": true,
      "Bubble": true,
      "CommandBubble": true,
      "Direction": true,
      "DwGame": true,
      "FadeOutInState": true,
      "Hero": true,
      "LoadingState": true,
      "Npc": true,
      "NpcType": true,
      "RoamingState": true,
      "TextBubble": true,
      "tiled": true,
      "TitleScreenState": true
   },
   "exported": [ "init", "BattleEntity" ]
}
