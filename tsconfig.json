{
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    //"noUnusedParameters": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    //"moduleResolution": "node",
    "module": "commonjs",
    "target": "es5",
    "outDir": "lib/",
    "sourceMap": true,
    "diagnostics": true
  }
}
